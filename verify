# For now only estimation and gso are added
if [ "$1" = "est" ]; then
  python3 sw-test/unit/est/genVectors.py
  echo "-----------------------------------------------------------------------------------------------------------------"
  ./simulate est
  echo "-----------------------------------------------------------------------------------------------------------------"
  python3 sw-test/unit/est/estCheck.py
elif [ "$1" = "gso" ]; then
  octave sw-test/unit/gso/gso.m
  echo "-----------------------------------------------------------------------------------------------------------------"
  ./simulate gso
  echo "-----------------------------------------------------------------------------------------------------------------"
  octave sw-test/unit/gso/gsoCheck.m
elif [ "$1" = "norm" ]; then
  python3 sw-test/unit/norm/genVectors.py
  echo "-----------------------------------------------------------------------------------------------------------------"
  ./simulate norm
  echo "-----------------------------------------------------------------------------------------------------------------"
  python3 sw-test/unit/norm/normCheck.py
elif [ "$1" = "theta" ]; then
  python3 sw-test/unit/theta/genVectors.py
  echo "-----------------------------------------------------------------------------------------------------------------"
  ./simulate theta
  echo "-----------------------------------------------------------------------------------------------------------------"
  python3 sw-test/unit/theta/thetaCheck.py
elif [ "$1" = "update" ]; then
  python3 sw-test/unit/updt/genVectors.py
  echo "-----------------------------------------------------------------------------------------------------------------"
  ./simulate update
fi
